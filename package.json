		"json-stringify-safe"
	],
	"jest": {
		"transform": {
			"^.+\\.(js|ts)$": "ts-jest"
		},
		"testPathIgnorePatterns": [
			"/node_modules/",
			"/dist/"
		],
		"setupTestFrameworkScriptFile": "<rootDir>/jest-setup.ts",
		"testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|js)$",
		"moduleFileExtensions": [
			"ts",
			"js",
			"json",
			"node"
		],
		"transformIgnorePatterns": [
			"<rootDir>/node_modules/(?!lodash-es)"
		]
	},
	"dependencies": {}
